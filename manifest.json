{
  "manifest_version": 3,
  "name": "Bookmark Sync Extension",
  "version": "1.0.0",
  "description": "Synchronize bookmarks across devices",
  "permissions": [
    "bookmarks",
    "identity",
    "storage",
    "notifications"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
